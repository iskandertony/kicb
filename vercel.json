{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": null,
  "buildCommand": "dotnet workload install wasm-tools && dotnet publish -c Release --output publish",
  "outputDirectory": "publish/wwwroot",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
